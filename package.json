{
  "private": true,
  "name": "@thret/home",
  "version": "0.0.1",
  "description": "UK based skate and streetwear clothing brand.",
  "repository": "github:thretclothing/home",
  "homepage": "https://thretclothing.com",
  "bugs": "https://github.com/thretclothing/home/issues",
  "author": "Thret Clothing Co & Contributers (https://github.com/thretclothing/home/graphs/contributors)",
  "scripts": {
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "start:ssr": "ng run thret:serve-ssr",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:ssr": "ngcc --tsconfig='./config/ts/tsconfig.server.json' && ng build --prod && ng run thret:server:production",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:headless": "ng test -c=headless",
    "test:headless:watch": "ng test -c=headless --watch",
    "lint": "ng lint",
    "prerender": "ng run thret:prerender",
    "serve:ssr": "node dist/thret/server/main.js",
    "postinstall": "ngcc --tsconfig='./config/ts/tsconfig.app.json'"
  },
  "dependencies": {
    "@angular/animations": "11.2.7",
    "@angular/common": "11.2.7",
